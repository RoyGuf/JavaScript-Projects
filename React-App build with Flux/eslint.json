{
  
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "no-lone-blocks": 0,
    "no-less-than": 0,
    "no-great-than": 0,
    "no-mixed-operators": 0,
    "jsx-quotes": 0
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}
